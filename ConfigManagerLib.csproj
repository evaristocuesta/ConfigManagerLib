<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <Version>1.0.0.0</Version>
    <FileVersion>1.0.0.0</FileVersion>
    <Authors>Evaristo Cuesta</Authors>
    <Company>Evaristo Cuesta</Company>
    <Description>Library to easily manage a config file</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>logo.png</PackageIcon>
    <PackageProjectUrl>https://github.com/evaristocuesta/ConfigManagerLib</PackageProjectUrl>
    <RepositoryUrl>https://github.com/evaristocuesta/ConfigManagerLib</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Config manager library</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SerializerLib" Version="1.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\SerializerLib\logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>
